<div class="row">
	<div class="col-md-12">
		<div class="box box-primary">
			<%= form_for [:admin, @data], url:  admin_order_path, method: :patch, :html=>{:multipart=>true} do |f| %>
				<div class="box-body">
					<div class="form-group">
						<label for="exampleInputPassword1">收货人</label>
            <p><%= @data.receiver_name%></p>
					</div>
					<div class="form-group">
						<label for="exampleInputPassword1">联系电话</label>
            <p><%= @data.receiver_phone%></p>
					</div>
					<div class="form-group">
						<label for="exampleInputPassword1">详细地址</label>
            <p><%= @data.receiver_address%></p>
					</div>
					<div class="form-group">
						<label for="exampleInputPassword1">订单金额</label>
						<%= f.text_field :total_fee, class: 'form-control'%>
					</div>
					<div class="form-group">
						<label for="exampleInputPassword1">运费</label>
						<%= f.text_field :fare, class: 'form-control'%>
					</div>
					
				</div>
				<div class="box-footer">
					<button class="btn btn-primary" type="submit">保存</button>
				</div>
			<% end %>

			
		</div> 
	</div> 
</div>

