<div class="row">
	<div class="col-md-12">
		<div class="box box-primary">
			<%= form_for [:admin, @data], url:  sent_save_admin_order_path, method: :patch, :html=>{:multipart=>true} do |f| %>
				<div class="box-body">
					<div class="form-group">
						<label for="exampleInputPassword1">快递公司</label>
						<%= f.select :express, options_for_select(Express.all.map{|e| [e.name, e.code]}, @data.express), {}, class: 'form-control'%>
					</div>
					<div class="form-group">
						<label for="exampleInputPassword1">快递单号</label>
						<%= f.text_field :express_number, class: 'form-control'%>
					</div>
					
				</div>
				<div class="box-footer">
					<button class="btn btn-primary" type="submit">保存</button>
				</div>
			<% end %>

			
		</div> 
	</div> 
</div>

